import java.util.*;
public class queue {
    public static void main(String[] args) {
        //Creating queue with the help of LinkedList
        Queue<Integer> q = new LinkedList<>();
        q.add(0);
        q.add(1);
        q.add(2);
        q.add(2);
        System.out.println("Oueue is: "+q);
        System.out.println(q.remove());
        System.out.println(q.peek());
    }
}
